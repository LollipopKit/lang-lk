Rect = {'width': 0, 'height': 0}

fn Rect:new() {
    o = {}
    for k, v in Rect {
      o[k] = v
    }
    rt o
}

fn Rect:square(len) {
    o = self:new()
    o.width = len
    o.height = len
    rt o
}

fn Rect:printArea() {
    print("Rect area: ", self.width * self.height)
}

fn Rect:set(width, height) {
    self.width = width
    self.height = height
}
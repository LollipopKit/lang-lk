shy resp, err = http.post(
    'http://httpbin.org/post', 
    {'accept': 'application/json'}, 
    '{"foo": "bar"}'
)
if err == nil {
    for k, v in resp.headers {
        print(k, v)
    }
}

shy resp, _ = http.req(
    'delete',
    'http://httpbin.org/delete',
    {'accept': 'application/json'},
    '{"foo": "bar"}'
)
if err == nil {
    print(resp.code, json.get(resp.body, 'json.foo'))
}
pass_fmt := '%s: ' + term.green + 'PASS' + term.nocolor
start_fmt := '%s: ' + term.cyan + 'START' + term.nocolor
fail_fmt := '%s: ' + term.red + 'FAIL' + term.nocolor

fn pass() {
    print(fmt(pass_fmt, name))
}
fn start() {
    print(fmt(start_fmt, name))
}
fn fail(msg) {
    print(fmt(fail_fmt, name))
    print(msg)
    os.exit(1)
}

name = 'list'
start()
import 'list'
a := new(list)
a:add(1, 2, 3, 4)
a:remove(2, 3)
a:remove_at(1)
a:insert(1, '7')
a:reverse()
if str(a) != '["7",1]' {
    fail(a)
}
pass()


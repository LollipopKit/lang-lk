import 'table'

dir := 'test/'
files, err := os.ls(dir)
if err != nil {
    print(err)
    os.exit(1)
}

skip_files := {
    'http_listen.lk'
}

for _, file in files {
    if !strs.contains(file, '.lkc') {
        if !table.contains(skip_files, file) {
            print(fmt('====== %s ======', file))
            do_file(dir + file)
        }
    }
}
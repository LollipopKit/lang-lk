import 'table'

dir := 'test/'
files, err := os.ls(dir)
if err != nil {
    print(err)
    os.exit(1)
}

skip_files := {
    'http_listen.lk'
}

for _, file in files {
    if not file:contains('.lkc') {
        _, have_val := skip_files:contains(file)
        if not have_val {
            print(fmt('====== %s ======', file))
            do_file(dir + file)
        }
    }
}